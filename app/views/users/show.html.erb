<% provide(:title, @user.name) %>

<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
        <h1>
            <%= gravatar_for @user %>
            <%= @user.name %>
        </h1>
        <br/>
        Created on: <%= @user.created_at %>
        <br/>
        Updated on: <%= @user.updated_at %>
        <br />
        <%= Time.now %>
    </section>
  </aside>
</div>
